{
  "expo": {
    "name": "NUMEROLOGIES",
    "slug": "numerologies",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/phucKhangLogo.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D0D0D"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nguyendaiphuc.phuckhangNumerologies"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/phucKhangLogo.png",
        "backgroundColor": "#0D0D0D"
      },
      "package": "com.nguyendaiphuc.phuckhangNumerologies"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c720bd05-5f0a-4fcb-99a8-301f5377a51d"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 31,
            "targetSdkVersion": 31,
            "buildToolsVersion": "31.0.0"
          },
          "ios": {
            "deploymentTarget": "13.0"
          }
        }
      ]
    ]
  }
}
